    <groundspeak:cache id="626742" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Goldenwood</groundspeak:name>
      <groundspeak:placed_by>gorgon9380</groundspeak:placed_by>
      <groundspeak:owner id="527062">gorgon9380</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
