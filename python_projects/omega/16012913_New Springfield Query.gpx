    <groundspeak:cache id="54233" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Potawatomi Trail of Death</groundspeak:name>
      <groundspeak:placed_by>eddie-the-razor</groundspeak:placed_by>
      <groundspeak:owner id="92461">eddie-the-razor</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
