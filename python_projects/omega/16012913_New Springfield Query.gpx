      <groundspeak:name>Aidyn's Science Lesson #1</groundspeak:name>
      <groundspeak:placed_by>mrori11</groundspeak:placed_by>
      <groundspeak:owner id="5133918">mrori11</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Regular</groundspeak:container>
      <groundspeak:attributes>
