    <groundspeak:cache id="4321743" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Guy Quotes #6</groundspeak:name>
      <groundspeak:placed_by>Mr.&amp;Mrs.F</groundspeak:placed_by>
      <groundspeak:owner id="7536955">The 0ld Goat</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
